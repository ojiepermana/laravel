{
    "name": "ojiepermana/laravel",
    "description": "Laravel utility package with blade directives, helpers, and services.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OjiePermana\\Laravel\\": "src/"
        },
        "files": [
            "src/Helpers/IndonesiaHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OjiePermana\\Laravel\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0|^12.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "OjiePermana\\Laravel\\LaravelServiceProvider"
            ],
            "aliases": {
                "GCS": "OjiePermana\\Laravel\\Facades\\GCS",
                "BNI": "OjiePermana\\Laravel\\Facades\\BNI"
            }
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10|^11|^12",
        "maatwebsite/excel": "^3.1",
        "google/cloud-storage": "^1.49",
        "league/flysystem": "^3.0"
    }
}